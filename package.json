{
  "name": "dentaflow",
  "version": "1.0.0",
  "description": "Application de gestion dentaire sécurisée",
  "main": "src/api/server.js",
  "scripts": {
    "start": "node src/api/server.js",
    "dev": "nodemon src/api/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "security": "npm audit --audit-level=high",
    "docs": "swagger-jsdoc -d docs/swagger-definition.js src/api/ > docs/swagger.json"
  },
  "keywords": ["dental", "healthcare", "devsecops"],
  "author": "Votre Équipe",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.18.0",
    "winston": "^3.0.0",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^5.0.0"
  }
}
